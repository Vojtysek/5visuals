{
  "name": "5visuals",
  "version": "1.1.0",
  "type": "module",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "moduleResolution": "bundler",
  "allowImportingTsExtensions": true,
  "verbatimModuleSyntax": true,
  "noEmit": true,
  "strict": true,
  "skipLibCheck": true,
  "noFallthroughCasesInSwitch": true,
  "noUncheckedIndexedAccess": true,
  "noUnusedLocals": false,
  "noUnusedParameters": false,
  "noPropertyAccessFromIndexSignature": false,
  "scripts": {
    "build": "tsup"
  },
  "peerDependencies": {
    "p5": "^1.6.0"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19"
  },
  "dependencies": {
    "@types/p5": "^1.7.6",
    "eslint": "^9.23.0",
    "p5": "^1.11.3",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0"
  }
}
